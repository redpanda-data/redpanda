"/v1/security/users": {
    "post": {
        "summary": "Create a user",
        "operationId": "create_user",
        "responses": {
            "200": {
                "description": "Create user"
            }
        }
    },
    "get": {
        "summary": "List users",
        "operationId": "list_users",
        "responses": {
            "200": {
                "description": "List users"
            }
        }
    }
},
