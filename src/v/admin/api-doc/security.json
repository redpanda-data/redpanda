        "operationId": "delete_user",
        "parameters": [
            {
                "name": "user",
                "in": "path",
                "required": true,
                "type": "string"
            }
        ],
        "responses": {
            "200": {
                "description": "Delete user"
            }
        }
    },
    "put": {
        "summary": "Update a user",
        "operationId": "update_user",
        "parameters": [
            {
                "name": "user",
                "in": "path",
                "required": true,
                "type": "string"
            }
        ],
        "responses": {
            "200": {
                "description": "Update user"
            }
        }
    }
}
