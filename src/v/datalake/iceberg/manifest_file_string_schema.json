      "doc": "Added rows count",
      "field-id": "512"
    },
    {
      "name": "existing_rows_count",
      "type": "long",
      "doc": "Existing rows count",
      "field-id": "513"
    },
    {
      "name": "deleted_rows_count",
      "type": "long",
      "doc": "Deleted rows count",
      "field-id": "514"
