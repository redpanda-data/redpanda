        }
      },
      "422": {
        "description": "Format Error",
        "schema": {
          "$ref": "#/definitions/error_body"
        }
      },
      "500": {
        "description": "Internal error",
        "schema": {
          "$ref": "#/definitions/error_body"
        }
      }
    }
  }
},
